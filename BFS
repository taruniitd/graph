#include <iostream>
#include <bits/stdc++.h>

void BFS()
{
    for(int i = 0; i < numVertex; i++)
      visited[i] = false;
    
    list<int> queue;
    queue.insert(root);
    
    while(queue != empty())
    {
      cur = queue.top();
      
      queue.pop();
      
      list<int>::iterator i;
      for( int k = i->begin(); k != i->end())
      {
        if(!visited[*i])
          queue->insert(*i);
      }
    }
    
}
